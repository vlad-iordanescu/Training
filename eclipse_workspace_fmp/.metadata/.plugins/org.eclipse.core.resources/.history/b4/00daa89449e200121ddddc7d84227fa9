package AnimalFarm;



public class AnimalCreation implements AnimalProp {

	public Animal createProduct(int ID) {
		Animal y = new Animal();
		
		return y;
	}
	
	public void registerProduct(int ID, String Name, int IDFather, int IDMother){
		Animal x = new Animal();
		x.ID = ID;
		x.Name = Name;
		x.IDFather = IDFather;
		x.IDMother = IDMother;
	}

}
