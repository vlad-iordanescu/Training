package AbstractWeaponsDealer;

public class WeaponsShop {

	public static MachineGunsFactory CreateM(String Name) {
		if (Name.equals("TommyGun"))
			return new TommyGun();
		return null;
	}

	public static Shotguns CreateS(String Name) {
		if (Name.equals("Beretta"))
			return new Beretta();
		return null;
	}

	public static HandGunsFactory CreateH(String Name) {
		if (Name.equals("Deagle"))
			return new Deagle();
		return null;
	}

}
