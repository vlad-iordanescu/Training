package AbstractWeaponsDealer;

public class WeaponsShop {
	
	public  MachineGuns Create1(String Name){
		if (Name.equals("TommyGun"))
			return new TommyGun();
	}
	
	public  Shotguns Create2(String Name){
		if (Name.equals("Beretta"))
			return new Beretta();
	}
	
	public  HandGuns Create3(String Name){
		if (Name.equals("Deagle"))
			return new Deagle();
	}
	
	public static void main(String args[]){
		
	}
}
