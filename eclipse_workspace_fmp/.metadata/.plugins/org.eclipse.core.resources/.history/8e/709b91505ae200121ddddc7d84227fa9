package AbstractWeaponsDealer;

public class WeaponsShop {

	public static Object Create (String Name) {
		Object returnobj;
		
		if (Name.equals("TommyGun")){
			returnobj = new TommyGun();
			return returnobj;
		}


		if (Name.equals("Beretta")){
			returnobj = new Beretta();
			return returnobj;
		}


		if (Name.equals("Deagle")){
			returnobj = new Deagle();
			return returnobj;
		}

	}

}
