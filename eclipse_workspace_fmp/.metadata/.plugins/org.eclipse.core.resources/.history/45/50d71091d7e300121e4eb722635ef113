package Lock;

public class myThread extends Thread{
	
	private Counter v= new Counter();
	
	public myThread (Counter v) {
		this.v = v;
	}
	
	public void run() throws InterruptedException {
	
		for(int i=0;i<25;i++) {
			v.increment();
			System.out.println(v.getValue());
			Thread.sleep(100);
		}
	}

}
