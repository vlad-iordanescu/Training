package AnimalFarm;

public interface AnimalProp {
	String getUsage();
	
	class Pig implements AnimalProp {
		public String getUsage()
		{
			return "Meat";
		}
	}
	
	class Chicken implements AnimalProp {
		public String getUsage()
		{
			return "Eggs";
		}
	}
	
	class Sheep implements AnimalProp {
		public String getUsage()
		{
			return "Wool";
		}
	}
	
}
