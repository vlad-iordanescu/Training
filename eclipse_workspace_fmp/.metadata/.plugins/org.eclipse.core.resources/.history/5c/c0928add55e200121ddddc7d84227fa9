package AbstractWeaponsDealer;

public class WeaponsShop {
	
	public static MachineGuns Create1(String Name){
		if (Name.equals("TommyGun"))
			return new TommyGun();
	}
	
	public static Shotguns Create2(String Name){
		if (Name.equals("Beretta"))
			return new Beretta();
	}
	
	public static HandGuns Create3(String Name){
		if (Name.equals("Deagle"))
			return new Deagle();
	}
	

}
