package AnimalFarm;
import java.util.*;
import AnimalFarm.Animal;

public class AnimalCreation implements AnimalProp {

	public int createProduct(int ID) {
		Animal x = new Animal;
	}
	
	public void registerProduct(int ID, String Name, int IDFather, int IDMother){
		Animal x = new Animal;
		x.ID = ID;
		x.Name = Name;
		x.IDFather = IDFather;
		x.IDMother = IDMother;
	}

}
