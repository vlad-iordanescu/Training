package AnimalFarm;



public class AnimalFactory {
	
	public AnimalProp CreateAnimal (String Name)
	{
		if (Usage.equals("Pig"))
			return new Pig();
		else if (Usage.equals("Chicken"))
			return new Chicken();
		else if (Usage.equals("Sheep"))
			return new Sheep();
		else
			return new Unknown();
	}


}
